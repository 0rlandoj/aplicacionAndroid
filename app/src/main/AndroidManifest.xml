    package="com.example.ragnarok">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivityCurrentSeason" />
        <activity android:name=".MainActivityCapitulos" />
        <activity android:name=".MainActivityEstadisticas" />
        <activity android:name=".MainActivityPerfilDelUsuario" />
        <activity android:name=".MainActivityInformacionDeLaSerie" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5768084728436674~5529192225" />
    </application>

</manifest>